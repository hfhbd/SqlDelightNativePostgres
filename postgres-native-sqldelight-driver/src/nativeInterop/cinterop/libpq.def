headers = libpq-fe.h
headerFilter = *
package = libpq
#staticLibraries = libpq.a
#libraryPaths = /opt/homebrew/opt/libpq/lib

compilerOpts = -I/home/linuxbrew/.linuxbrew/opt/libpq/include -I/opt/homebrew/opt/libpq/include -I/usr/local/opt/libpq/include
